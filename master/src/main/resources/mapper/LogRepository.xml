<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.monkey.mapper.LogRepository">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.monkey.core.entity.Log">
        <result column="id" property="id" />
        <result column="logDescription" property="logDescription" />
        <result column="actionArgs" property="actionArgs" />
        <result column="userName" property="userName" />
        <result column="className" property="className" />
        <result column="method" property="method" />
        <result column="ip" property="ip" />
        <result column="creationTime" property="creationTime" />
        <result column="succeed" property="succeed" />
        <result column="message" property="message" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, logDescription, actionArgs, userName, className, method, ip, creationTime, succeed, message
    </sql>

</mapper>
